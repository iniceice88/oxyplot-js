{
  "name": "oxyplot-js-renderers",
  "type": "module",
  "version": "2.1.2003",
  "packageManager": "pnpm@8.14.0",
  "author": "inice88",
  "description": "renderers for oxyplot-js",
  "homepage": "https://github.com/iniceice88/oxyplot-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iniceice88/oxyplot-js.git",
    "directory": "source"
  },
  "keywords": [
    "oxyplot",
    "oxyplot-js"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm type-check && vite build",
    "type-check": "tsc --noEmit"
  },
  "main": "./dist/oxyplot-js-renderers.umd.cjs",
  "module": "./dist/oxyplot-js-renderers.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/oxyplot-js-renderers.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/oxyplot-js-renderers.umd.cjs"
      }
    }
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.2",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.1",
    "jspdf": "^2.5.1",
    "oxyplot-js": "2.1.2001"
  }
}
