{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": true,
    "declarationMap": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "types": [],
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
